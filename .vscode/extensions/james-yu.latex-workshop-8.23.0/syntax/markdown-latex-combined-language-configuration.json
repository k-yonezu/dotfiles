{
	"comments": {
		"blockComment": [
			"<!--",
			"-->"
		]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["[", ")"],
		["(", "]"],
		["\\left(", "\\right)"],
		["\\left(", "\\right."],
		["\\left.", "\\right)"],
		["\\left[", "\\right]"],
		["\\left[", "\\right."],
		["\\left.", "\\right]"],
		["\\left\\{", "\\right\\}"],
		["\\left\\{", "\\right."],
		["\\left.", "\\right\\}"],
		["\\left<", "\\right>"],
		["\\bigl(", "\\bigr)"],
		["\\bigl[", "\\bigr]"],
		["\\bigl\\{", "\\bigr\\}"],
		["\\Bigl(", "\\Bigr)"],
		["\\Bigl[", "\\Bigr]"],
		["\\Bigl\\{", "\\Bigr\\}"],
		["\\biggl(", "\\biggr)"],
		["\\biggl[", "\\biggr]"],
		["\\biggl\\{", "\\biggr\\}"],
		["\\Biggl(", "\\Biggr)"],
		["\\Biggl[", "\\Biggr]"],
		["\\Biggl\\{", "\\Biggr\\}"]
	],
	"autoClosingPairs": [
		["\\begin{", ""],
		["\\left(", ""],
		["\\left[", ""],
		["\\left{", ""],
		["\\bigl(", ""],
		["\\bigl[", ""],
		["\\bigl{", ""],
		["\\Bigl(", ""],
		["\\Bigl[", ""],
		["\\Bigl{", ""],
		["\\biggl(", ""],
		["\\biggl[", ""],
		["\\biggl{", ""],
		["\\Biggl(", ""],
		["\\Biggl[", ""],
		["\\Biggl{", ""],
		["\\(", ""],
		["\\[", ""],
		["\\{", ""],
		["@(", ""],
		["@[", ""],
		["@{", ""],
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["`", "`"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["$", "$"],
		["`", "`"],
		["_", "_"],
		["*", "*"]
	],
	"indentationRules": {
		"increaseIndentPattern": "\\\\begin{(?!document)([^}]*)}(?!.*\\\\end{\\1})",
		"decreaseIndentPattern": "^\\s*\\\\end{(?!document)"
	},
	"autoCloseBefore": ";:.,=}])>\\` \n\t$",
	"wordPattern": "([^\\s`'\"~_!?|$#@%^&*\\-=+;:,.<>(){}[\\]\\/]+)",
	"folding": {
		"offSide": true,
		"markers": {
			"start": "^\\s*<!--\\s*#?region\\b.*-->",
			"end": "^\\s*<!--\\s*#?endregion\\b.*-->"
		}
	}
}
