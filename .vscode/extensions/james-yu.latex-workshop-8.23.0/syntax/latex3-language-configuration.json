{
	"comments": {
		"lineComment": "%"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["[", ")"],
		["(", "]"],
		["\\left(", "\\right)"],
		["\\left(", "\\right."],
		["\\left.", "\\right)"],
		["\\left[", "\\right]"],
		["\\left[", "\\right."],
		["\\left.", "\\right]"],
		["\\left\\{", "\\right\\}"],
		["\\left\\{", "\\right."],
		["\\left.", "\\right\\}"],
		["\\left<", "\\right>"],
		["\\bigl(", "\\bigr)"],
		["\\bigl[", "\\bigr]"],
		["\\bigl\\{", "\\bigr\\}"],
		["\\Bigl(", "\\Bigr)"],
		["\\Bigl[", "\\Bigr]"],
		["\\Bigl\\{", "\\Bigr\\}"],
		["\\biggl(", "\\biggr)"],
		["\\biggl[", "\\biggr]"],
		["\\biggl\\{", "\\biggr\\}"],
		["\\Biggl(", "\\Biggr)"],
		["\\Biggl[", "\\Biggr]"],
		["\\Biggl\\{", "\\Biggr\\}"]
	],
	"autoClosingPairs": [
		["\\begin{", ""],
		["\\left(", ""],
		["\\left[", ""],
		["\\left{", ""],
		["\\bigl(", ""],
		["\\bigl[", ""],
		["\\bigl{", ""],
		["\\Bigl(", ""],
		["\\Bigl[", ""],
		["\\Bigl{", ""],
		["\\biggl(", ""],
		["\\biggl[", ""],
		["\\biggl{", ""],
		["\\Biggl(", ""],
		["\\Biggl[", ""],
		["\\Biggl{", ""],
		["\\(", ""],
		["\\[", ""],
		["\\{", ""],
		["@(", ""],
		["@[", ""],
		["@{", ""],
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["`", "'"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "'"],
		["$", "$"]
	],
	"indentationRules": {
		"increaseIndentPattern": "\\\\begin{(?!document)([^}]*)}(?!.*\\\\end{\\1})",
		"decreaseIndentPattern": "^\\s*\\\\end{(?!document)"
	},
	"autoCloseBefore": ";:.,=}])>\\` \n\t$",
	"wordPattern": "(__)|(\\*\\*)|(\\.\\.\\.)|([^\\s`'\"~_!?|$#@%^&*\\-=+;:,.<>(){}[\\]\\\\\\/]{2,})"
}
